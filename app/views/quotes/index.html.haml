%p#notice= notice
%h1 Quotes
%table
  %thead
    %tr
      %th Date
      %th Price
      %th Financial asset
      %th{:colspan => "3"}
  %tbody
    - @quotes.each do |quote|
      %tr
        %td= quote.date
        %td= quote.price
        %td= quote.financial_asset_id
        %td= link_to 'Show', quote
        %td= link_to 'Edit', edit_quote_path(quote)
        %td= link_to 'Destroy', quote, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Quote', new_quote_path
