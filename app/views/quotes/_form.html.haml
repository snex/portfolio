= bootstrap_form_with(model: quote, local: true) do |form|
  - if quote.errors.any?
    #error_explanation
      %h2
        = pluralize(quote.errors.count, "error")
        prohibited this quote from being saved:
      %ul
        - quote.errors.full_messages.each do |message|
          %li= message
  .field
    = form.label :date
    = form.date_select :date
  .field
    = form.label :price
    = form.text_field :price
  .field
    = form.label :financial_asset_id
    = form.text_field :financial_asset_id
  .actions
    = form.submit
