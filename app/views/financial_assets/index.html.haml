%p#notice= notice
%h1 Financial Assets
%table
  %thead
    %tr
      %th Ticker symbol
      %th{:colspan => "3"}
  %tbody
    - @financial_assets.each do |financial_asset|
      %tr
        %td= financial_asset.ticker_symbol
        %td= link_to 'Show', financial_asset
        %td= link_to 'Edit', edit_financial_asset_path(financial_asset)
        %td= link_to 'Destroy', financial_asset, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Financial Asset', new_financial_asset_path
